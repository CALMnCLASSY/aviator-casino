{
  "version": 2,
  "builds": [
    {
      "src": "classybet-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "classybet/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/classybet/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}