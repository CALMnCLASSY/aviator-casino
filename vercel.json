{
  "version": 2,
  "builds": [
    {
      "src": "classybet-backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/health",
      "dest": "/classybet-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/classybet/$1"
    }
  ]
}