{
  "version": 2,
  "functions": {
    "classybet-backend/server.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/classybet-backend/server.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/classybet-backend/server.js"
    },
    {
      "source": "/profile/(.*)",
      "destination": "/classybet-backend/server.js"
    },
    {
      "source": "/health",
      "destination": "/classybet-backend/server.js"
    }
  ]
}