{
  "version": 2,
  "builds": [
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/simple.js",
      "use": "@vercel/node"
    },
    {
      "src": "classybet/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/db-test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/login-test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/simple.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/simple.js"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/api/simple.js"
    },
    {
      "src": "/health",
      "dest": "/api/simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/classybet/$1"
    }
  ]
}